        <div class='inner' id='mainblogspothoatranweb'>
          <div class='main_container'>
            <div class='main-col search-home' id='main_container'>
             <b:if cond='data:blog.url == data:blog.homepageUrl'>
               <h1 style='display:none'><data:blog.title/></h1>
                <div class='news-box  base-box scrolling-box-wrap'>
<b:section id='news-box-slide' showaddelement='yes'>
  <b:widget id='HTML5' locked='false' title='' type='HTML'>
    <b:widget-settings>
      <b:widget-setting name='content'><![CDATA[<header class='nb-header'>
                    <h3 class='nb-title'>
                      <a href='/search/label/viec-lam' title='Việc làm lập trình'>
                        Việc làm mới nhất
                      </a>
